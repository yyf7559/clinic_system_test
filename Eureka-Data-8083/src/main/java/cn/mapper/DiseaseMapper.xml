<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mapper.DiseaseMapper">
    <insert id="addDisease">
        insert into prescription_disease (id,prescriptionId,diseaseId)
        values(null,#{prescriptionDisease.prescriptionId},#{prescriptionDisease.diseaseId})
    </insert>
    <select id="findDisease" resultType="cn.entity.Disease">
        select * from Disease
    </select>
</mapper>