<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mapper.CheckMapper">
    <insert id="addPreCheck">
        insert into prescription_check (id,prescriptionId,checkId,`number`,`count`,pmark)
        values(null,#{prescriptionCheck.prescriptionId},#{prescriptionCheck.checkId},#{prescriptionCheck.number},#{prescriptionCheck.count},#{prescriptionCheck.pmark})
    </insert>
</mapper>