<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Manufacturer" table="MANUFACTURER" schema="SCOTT">
        <id name="id">
            <column name="ID" sql-type="number(11)" precision="11"/>
        </id>
        <property name="manufacturername">
            <column name="MANUFACTURERNAME" sql-type="varchar2(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>